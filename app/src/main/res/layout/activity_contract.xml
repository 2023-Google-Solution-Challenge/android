                android:layout_height="wrap_content"
                android:background="@drawable/box_contract_select"
                android:textSize="12sp"
                android:hint="시작시간"
                android:textColor="@color/font"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_contract_time"
                app:layout_constraintEnd_toStartOf="@+id/et_contract_etime"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                android:layout_marginTop="6dp"
                android:layout_marginRight="35dp"
                android:paddingHorizontal="15dp"
                android:paddingVertical="12dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="~"
                android:textColor="@color/gray2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/et_contract_stime"
                app:layout_constraintBottom_toBottomOf="@id/et_contract_stime"/>
            <TextView
