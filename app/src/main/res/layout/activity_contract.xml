                android:textSize="12sp"
                android:textColor="@color/white"
                android:background="@drawable/box_stroke_gray_half_round_l"
                app:layout_constraintTop_toBottomOf="@id/tv_contract_break"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/btn_contract_break_no"
                android:layout_marginTop="6dp"/>
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn_contract_break_no"
                android:layout_width="0dp"
                android:layout_height="36dp"
                android:text="휴게시간 없음"
                android:textSize="12sp"
                android:textColor="@color/gray1"
                android:background="@drawable/box_stroke_gray_half_round_r"
                app:layout_constraintTop_toTopOf="@id/btn_contract_break_yes"
                app:layout_constraintStart_toEndOf="@id/btn_contract_break_yes"
                app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                android:id="@+id/et_contract_sbreak"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/box_contract_select"
                android:textSize="12sp"
                android:hint="시작시간"
                android:textColor="@color/font"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/btn_contract_break_yes"
                app:layout_constraintEnd_toStartOf="@+id/et_contract_ebreak"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                android:layout_marginTop="9dp"
                android:layout_marginRight="35dp"
                android:paddingHorizontal="15dp"
                android:paddingVertical="12dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="~"
                android:textColor="@color/gray2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/et_contract_sbreak"
                app:layout_constraintBottom_toBottomOf="@id/et_contract_sbreak"/>
            <TextView
                android:id="@+id/et_contract_ebreak"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/box_contract_select"
                android:textSize="12sp"
                android:hint="종료시간"
                android:textColor="@color/font"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/et_contract_sbreak"
                app:layout_constraintStart_toEndOf="@id/et_contract_sbreak"
