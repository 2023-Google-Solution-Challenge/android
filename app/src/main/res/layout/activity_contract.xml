                android:layout_height="wrap_content"
                android:background="@drawable/box_contract_date"
                android:textSize="12sp"
                android:hint="시작일 입력"
                android:textColor="@color/font"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_contract_period"
                app:layout_constraintEnd_toStartOf="@+id/et_contract_periode"
