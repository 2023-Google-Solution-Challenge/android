                android:layout_marginTop="6dp"
                android:layout_marginRight="35dp"
                android:paddingHorizontal="15dp"
                android:paddingVertical="12dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="~"
                android:textColor="@color/gray2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/et_contract_periods"
                app:layout_constraintBottom_toBottomOf="@id/et_contract_periods"/>
            <TextView
                android:id="@+id/et_contract_periode"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/box_contract_date"
                android:textSize="12sp"
                android:hint="종료일 입력"
                android:textColor="@color/font"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/et_contract_periods"
                app:layout_constraintStart_toEndOf="@id/et_contract_periods"
                android:paddingHorizontal="15dp"
                android:paddingVertical="12dp"/>

            <TextView
                android:id="@+id/tv_contract_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="근로시간"
                android:textColor="@color/font"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/et_contract_periods"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="24dp"/>
            <TextView
