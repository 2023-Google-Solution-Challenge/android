<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingTop="13dp">
    <ImageView
        android:id="@+id/btn_contract_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/btn_all_back"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginLeft="24dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="근로 계약서"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_contract_back"
        app:layout_constraintBottom_toBottomOf="@id/btn_contract_back" />
    <View
        android:id="@+id/v1_contract"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/gray3"
        app:layout_constraintTop_toBottomOf="@+id/btn_contract_back"
        android:layout_marginTop="13dp"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/v1_contract"
        app:layout_constraintBottom_toBottomOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_contract"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/v1_contract"
            app:layout_constraintStart_toStartOf="parent"
            android:paddingHorizontal="24dp"
            android:paddingVertical="24dp">
            <TextView
                android:id="@+id/tv1_contract"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="근로자 정보"
                android:textColor="@color/font"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:id="@+id/tv_contract_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="성명"
                android:textColor="@color/font"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv1_contract"
                android:layout_marginTop="24dp"
                android:layout_marginLeft="5dp"/>
            <EditText
                android:id="@+id/et_contract_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/box_stroke_all"
                android:textSize="12sp"
                android:inputType="text"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_contract_name"
                android:layout_marginTop="6dp"
                android:paddingHorizontal="15dp"
                android:paddingVertical="12dp"/>
            <TextView
                android:id="@+id/tv_contract_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="주소"
                android:textColor="@color/font"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/et_contract_name"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="24dp"/>
            <EditText
                android:id="@+id/et_contract_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/box_stroke_all"
                android:textSize="12sp"
                android:inputType="text"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_contract_address"
                android:layout_marginTop="6dp"
                android:paddingHorizontal="15dp"
                android:paddingVertical="12dp"/>
            <TextView
                android:id="@+id/tv_contract_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="연락처"
                android:textColor="@color/font"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/et_contract_address"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="24dp"/>
            <EditText
                android:id="@+id/et_contract_phone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/box_stroke_all"
                android:textSize="12sp"
                android:hint="숫자만 입력"
                android:inputType="number"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_contract_phone"
                android:layout_marginTop="6dp"
                android:paddingHorizontal="15dp"
                android:paddingVertical="12dp"/>
            <TextView
                android:id="@+id/tv_contract_sign"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="서명"
                android:textColor="@color/font"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/et_contract_phone"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="24dp"/>
            <com.kyanogen.signatureview.SignatureView
                xmlns:sign="http://schemas.android.com/apk/res-auto"
                android:id="@+id/et_contract_sign"
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:background="@drawable/box_stroke_all"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_contract_sign"
                android:layout_marginTop="6dp"
                android:paddingHorizontal="15dp"
                android:paddingVertical="12dp"
                app:penColor="@android:color/black"
                sign:enableSignature="true" />
<!--            <EditText-->
<!--                android:id="@+id/et_contract_sign"-->
<!--                android:layout_width="match_parent"-->
<!--                android:layout_height="120dp"-->
<!--                android:background="@drawable/box_stroke_all"-->
<!--                android:textSize="12sp"-->
<!--                android:inputType="text"-->
<!--                app:layout_constraintStart_toStartOf="parent"-->
<!--                app:layout_constraintTop_toBottomOf="@id/tv_contract_sign"-->
<!--                android:layout_marginTop="6dp"-->
<!--                android:paddingHorizontal="15dp"-->
<!--                android:paddingVertical="12dp"/>-->
            <TextView
                android:id="@+id/tv_contract_shop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="사업주"
                android:textColor="@color/font"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/et_contract_sign"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="24dp"/>
            <EditText
                android:id="@+id/et_contract_shop"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/box_stroke_all"
                android:backgroundTint="@color/gray3"
                android:textSize="12sp"
                android:inputType="text"
                android:text="Cafe peach"
                android:textColor="@color/font"
                android:enabled="false"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_contract_shop"
                android:layout_marginTop="6dp"
                android:paddingHorizontal="15dp"
                android:paddingVertical="12dp"/>

            <TextView
                android:id="@+id/tv2_contract"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="근무조건"
                android:textColor="@color/font"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/et_contract_shop"
                android:layout_marginTop="24dp"/>
            <TextView
                android:id="@+id/tv_contract_period"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="근로기간"
                android:textColor="@color/font"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv2_contract"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="24dp"/>
            <TextView
