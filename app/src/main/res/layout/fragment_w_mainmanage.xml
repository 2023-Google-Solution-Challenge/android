<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/sliding_wmanage"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:gravity="bottom">
    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingHorizontal="24dp"
        android:paddingTop="24dp">
        <TextView
            android:id="@+id/tv_wmanage_ongoing"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="진행중"
            android:textSize="16sp"
            android:textColor="@color/font"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
        <TextView
            android:id="@+id/tv_wmanage_finish"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="근무완료"
            android:textSize="16sp"
            android:textColor="@color/gray1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/tv_wmanage_ongoing"
            android:layout_marginLeft="36dp"/>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rv_wmange"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            app:layout_constraintTop_toBottomOf="@id/tv_wmanage_ongoing"
            android:layout_marginTop="24dp"
            tools:listitem="@layout/item_manage"/>


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

